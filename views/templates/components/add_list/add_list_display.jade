mixin add-list-display(list_items, display_id)
    each list_item in list_items
        li.list-group-item
            | #{list_item}
            span.action
                a.remove-list-item(href="#")
                    i.fas.fa-trash
        //li.list-group-item
            | http//www.facebook.com
            span.action
                a(href="#")
                    i.fas.fa-trash
                            
- if(!list_items){
-     list_items = [];
- }
                            
+add-list-display(list_items)