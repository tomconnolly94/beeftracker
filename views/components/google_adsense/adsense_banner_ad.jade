mixin adsense-banner-ad()
    - if(!dev_env){
        div(align="center")
            script(async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js")
            ins(class="adsbygoogle",
                style="display:block",
                data-ad-client="ca-pub-2566324736007588",
                data-ad-slot="2354175800",
                data-ad-format="auto")
            script.
                (adsbygoogle = window.adsbygoogle || []).push({});
    - }